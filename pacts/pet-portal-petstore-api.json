{
  "consumer": {
    "name": "pet-portal"
  },
  "interactions": [
    {
      "description": "a request to create a pet",
      "providerState": "pets baseline",
      "request": {
        "body": {
          "id": 101,
          "name": "Fluffy Pact Cat",
          "photoUrls": [
            "https://example.pact/pets/fluffy.png"
          ],
          "status": "available"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "$.body.id": {
            "match": "type"
          },
          "$.body.name": {
            "match": "type"
          },
          "$.body.photoUrls": {
            "match": "type",
            "min": 1
          },
          "$.body.status": {
            "match": "regex",
            "regex": "available|pending|sold"
          }
        },
        "method": "POST",
        "path": "/v2/pet"
      },
      "response": {
        "body": {
          "id": 101,
          "name": "Fluffy Pact Cat",
          "photoUrls": [
            "https://example.pact/pets/fluffy.png"
          ],
          "status": "available"
        },
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "matchingRules": {
          "$.body.id": {
            "match": "type"
          },
          "$.body.name": {
            "match": "type"
          },
          "$.body.photoUrls": {
            "match": "type",
            "min": 1
          },
          "$.body.status": {
            "match": "regex",
            "regex": "available|pending|sold"
          },
          "$.headers['Content-Type']": {
            "match": "regex",
            "regex": "application\\/json(?:;\\s?charset=utf-8)?"
          }
        },
        "status": 200
      }
    },
    {
      "description": "a request to fetch an existing pet",
      "providerState": "pet with id 101 exists",
      "request": {
        "method": "GET",
        "path": "/v2/pet/101"
      },
      "response": {
        "body": {
          "id": 101,
          "name": "Fluffy Pact Cat",
          "photoUrls": [
            "https://example.pact/pets/fluffy.png"
          ],
          "status": "available"
        },
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "matchingRules": {
          "$.body.id": {
            "match": "type"
          },
          "$.body.name": {
            "match": "type"
          },
          "$.body.photoUrls": {
            "match": "type",
            "min": 1
          },
          "$.body.status": {
            "match": "regex",
            "regex": "available|pending|sold"
          },
          "$.headers['Content-Type']": {
            "match": "regex",
            "regex": "application\\/json(?:;\\s?charset=utf-8)?"
          }
        },
        "status": 200
      }
    },
    {
      "description": "a request for a missing pet",
      "providerState": "no pet with id 404",
      "request": {
        "method": "GET",
        "path": "/v2/pet/404"
      },
      "response": {
        "body": {
          "status": 404,
          "title": "Resource Not Found",
          "type": "/problems/not-found"
        },
        "headers": {
          "Content-Type": "application/problem+json"
        },
        "matchingRules": {
          "$.body.status": {
            "match": "type"
          }
        },
        "status": 404
      }
    }
  ],
  "metadata": {
    "pact-go": {
      "version": "2.4.2"
    },
    "pactRust": {
      "ffi": "0.4.28",
      "mockserver": "1.2.16",
      "models": "1.3.5"
    },
    "pactSpecification": {
      "version": "2.0.0"
    }
  },
  "provider": {
    "name": "petstore-api"
  }
}